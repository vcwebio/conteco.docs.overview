#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export CONTECO_DC_GLOBAL_STACKS="docs.overview"
export CONTECO_DC_GLOBAL_STACKNAMES="docs"
export CONTECO_DC_GLOBAL_BOOTSTACKS=""
export CONTECO_DC_GLOBAL_RUNSTACKS="docs"
export CONTECO_DC_GLOBAL_VOLUME_LIST=""
export CONTECO_DC_GLOBAL='$CONTECO_DC_GLOBAL_SERVICE_NAME,$CONTECO_DC_GLOBAL_DOCS_OVERVIEW_NAME,$CONTECO_DC_DOCS_OVERVIEW_ENVIRONMENT,$CONTECO_DC_DOCS_OVERVIEW_COMMAND,$CONTECO_DC_DOCS_OVERVIEW_VOLUMES,$CONTECO_DC_DOCS_OVERVIEW_NETWORKS,$CONTECO_DC_DOCS_OVERVIEW_PORTS,$CONTECO_DC_DOCS_OVERVIEW_DEPLOY_MODE,$CONTECO_DC_DOCS_OVERVIEW_DEPLOY_REPLICAS,$CONTECO_DC_DOCS_OVERVIEW_DEPLOY_RESOURCES_LIMITS_CPUS,$CONTECO_DC_DOCS_OVERVIEW_DEPLOY_RESOURCES_RESERVATIONS_CPUS,$CONTECO_DC_DOCS_OVERVIEW_DEPLOY_RESOURCES_LIMITS_MEMORY,$CONTECO_DC_DOCS_OVERVIEW_DEPLOY_RESOURCES_RESERVATIONS_MEMORY'
# END CONFIGURATION

# START GLOBAL SETTINGS
export CONTECO_DC_GLOBAL_SERVICE_NAME="${CONTECO_TYPE}"
export CONTECO_DC_GLOBAL_IMAGE_NAME="${CONTECO_TYPE}.${CONTECO_NAME}:${CONTECO_TAG}"
export CONTECO_DC_DOCS_OVERVIEW_ENVIRONMENT=
export CONTECO_DC_DOCS_OVERVIEW_COMMAND="'usleep 500000000'"
export CONTECO_DC_DOCS_OVERVIEW_PORTS=
export CONTECO_DC_DOCS_OVERVIEW_VOLUMES=
export CONTECO_DC_DOCS_OVERVIEW_NETWORKS=
export CONTECO_DC_DOCS_OVERVIEW_DEPLOY_LABELS=
export CONTECO_DC_DOCS_OVERVIEW_DEPLOY_MODE="replicated"
export CONTECO_DC_DOCS_OVERVIEW_DEPLOY_REPLICAS="1"
export CONTECO_DC_DOCS_OVERVIEW_DEPLOY_RESOURCES_LIMITS_CPUS="0.75"
export CONTECO_DC_DOCS_OVERVIEW_DEPLOY_RESOURCES_RESERVATIONS_CPUS="0.0"
export CONTECO_DC_DOCS_OVERVIEW_DEPLOY_RESOURCES_LIMITS_MEMORY="128m"
export CONTECO_DC_DOCS_OVERVIEW_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="5m"
# END GLOBAL SETTINGS

# START SETTINGS docs.overview
# END SETTINGS docs.overview

# START TOPLEVEL SETTINGS
export CONTECO_DC_GLOBAL_VOLUMES=
export CONTECO_DC_GLOBAL_NETWORKS=
# END TOPLEVEL SETTINGS
